    "tti-polyfill": "^0.2.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test"
    }
